.popupMask {
	position: absolute;
	z-index: 1000;
	background-color: #000;
	display: none;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	min-width: 100px;
	min-height: 100px;
}

.popupWindow {
	display: none;
	position: absolute;
	background-color: #FFF;
	z-index: 10000;
	border: 1px solid #000000;
	top: 100px;
	left: 100px;
}

.loadingBg {
	position: absolute;
	z-index: 20000;
	background-color: #FFF;
	display: none;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	min-width: 100px;
	min-height: 100px;
}

.loadingWindow {
	display: none;
	position: absolute;
	z-index: 30000;
	top: 50%;
	left: 50%;
}