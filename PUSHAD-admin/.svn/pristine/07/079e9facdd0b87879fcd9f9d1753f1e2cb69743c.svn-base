@charset "utf-8";
/*Reset*/
*{margin:0;padding:0}
body{ background-color:#fff;*word-break:break-all;-ms-word-break:break-all;font-family:돋움, 돋움체, sans-serif;font-size:12px;}
body,input,button{font-size:12px;line-height:1.5em;font-family:"",Dotum,Helvetica,AppleGothic,Sans-serif;color:#383d41}
h1,h2,h3,h4,h5,h6{font-size:12px;text-align:left}
img,fieldset,iframe{border:0 none; vertical-align:bottom}
input{vertical-align:middle;margin:0 2px 0 2px; border:1px solid #CBCBCB}
input.clear{border:none}
select{vertical-align:middle;}
li{list-style:none}
em,address{font-style:normal}
button,label{cursor:pointer;_cursor /**/:hand}
button *{position:relative}
button img{left:-3px;*left:auto}
textarea{overflow:auto}
html:first-child select{height:20px;padding-right:6px}
option{padding-right:6px}
a{color:#2f3743;text-decoration:none}
a:hover{color:#390;text-decoration:none}
hr{ display:none}
caption, legend {display:none; }
.clear{ clear:both}
.yNone{ border:0}
/*̺⺻*/
table {border-collapse:collapse; word-break:break-all;}
td a:link {text-decoration:none; color:#707070;}
td a:visited {text-decoration:none;	color:#525252;}
td a:hover {color:#ff2d3a;}
td img {vertical-align:middle;	padding-left:3px;}
td input {border-width:1; border:1px solid #ccced7;	padding:1px 2px 3px;}
td select {margin-top:0px;	vertical-align:center;	font-size:11px;	line-spacing:-1; color:#525252;}
td textarea {border-width:1; border:1px solid #ccced7;}
td p {padding-top:3px;}

.invisible {visibility:hidden; position:absolute; left:-10000px; top:0; height:0 !important; font-size:0 !important; line-height:0 !important; }



